    var svg = '<div class="o-ratio o-ratio"><div class="o-ratio__content"><svg class="clip-svg" viewBox="0 0 362 318" overflow="visible"><image class="svg-images" width="' + imagenWidth + '" height="' + imageHeight + '" xlink:href="' + url + '" clip-path="url(#' + id_mascara + ')" /><clipPath id="' + id_mascara + '" >' + poligono + '</clipPath></svg></div></div>';
    //$(bcp_formas[obj]).append(svg);
    $(bcp_formas[obj]).replaceWith(svg);

});
$("#enviar_registro_api").click(function () {
    var nombre = $("#txtNombreCompleto");
    var empresa = $("#txtEmpresa");
    var email = $("#txtEmail");
    var celular = $("#txtCelular");
    var captcha = $("#txtCaptcha");

    nombre.css("border-bottom", "1px solid #ccc");
    empresa.css("border-bottom", "1px solid #ccc");
    email.css("border-bottom", "1px solid #ccc");
    celular.css("border-bottom", "1px solid #ccc");
    captcha.css("border-bottom", "1px solid #ccc");

    $("#formulario_aviso").text("Debes llenar todos los campos del formulario ");
    $("#formulario_aviso").css("display", "none");
    if (nombre.val().trim() == "") {
        $("#formulario_aviso").css("display", "block");
        nombre.css("border-bottom", "3px solid red");
        return false;
    };
    if (!alfa(nombre.val().trim())) {
        $("#formulario_aviso").css("display", "block");
        $("#formulario_aviso").text("El valor del campo nombre debe contener solo letras");
        nombre.css("border-bottom", "3px solid red");
        return false;
    }
    if (empresa.val().trim() == "") {
        $("#formulario_aviso").css("display", "block");
        empresa.css("border-bottom", "3px solid red");
        return false;
    };
    if (!alfanumerico(empresa.val().trim())) {
        $("#formulario_aviso").css("display", "block");
        $("#formulario_aviso").text("El valor del campo empresa no debe contener caracteres especiales");
        empresa.css("border-bottom", "3px solid red");
        return false;
    }
    if (email.val().trim() == "") {
        $("#formulario_aviso").css("display", "block");
        email.css("border-bottom", "3px solid red");
        return false;
    };
    if (!val_email(email.val().trim())) {
        $("#formulario_aviso").css("display", "block");
        $("#formulario_aviso").text("No se reconoce como un correo electrnico valido");
        email.css("border-bottom", "3px solid red");
        return false;
    }
    if (celular.val().trim() == "") {

        $("#formulario_aviso").css("display", "block");
        celular.css("border-bottom", "3px solid red");
        return false;
    };
    if (!numerico(celular.val().trim())) {
        $("#formulario_aviso").css("display", "block");
        $("#formulario_aviso").text("El valor del campo celular debe ser numerico");
        celular.css("border-bottom", "3px solid red");
        return false;
    }
    if (captcha.val().trim() == "") {
        $("#formulario_aviso").css("display", "block");
        captcha.css("border-bottom", "3px solid red");
        return false;
    };
    if (!alfanumerico(captcha.val().trim())) {
        $("#formulario_aviso").css("display", "block");
        $("#formulario_aviso").text("El valor del campo captcha no debe contener caracteres especiales");
        captcha.css("border-bottom", "1px solid red");
        return false;
    }
    var formulario = {
        nombreCompleto: nombre.val().trim(),
        empresa: empresa.val().trim(),
        email: email.val().trim(),
        celular: celular.val().trim(),
        captcha: captcha.val().trim()
    };
    var modal = $("#registro_modal");
    var modal_text = $("#registro_modal_mensaje");
    var modal_form = $("#registro_formulario");
    var aviso = $("#formulario_aviso");
    var load = $("#enviar_registro_load");
    var btn = $("#enviar_registro_api");

    btn.css("display", "none");
    load.css("display", "block");

    $.ajax({
        type: "POST",
        url: "/Desarrollo/EnviarRegistro",
        dataType: "json",
        data: formulario,
        error: function (XMLHttpRequest, textStatus, errorThrown) {
            console.log("Solicitud: " + XMLHttpRequest.toString() + "\n\nEstado: " + textStatus + "\n\nError: " + errorThrown);
            aviso.css("display", "block");
            aviso.text("Ocurrio un error al enviar su peticion, porfavor intente mas tarde.");
            btn.css("display", "inline-block");
            load.css("display", "none");
        },
        complete: function (jqXHR, status) {
            console.log("completo: " + status + "\n\nRespuesta: " + jqXHR.responseText);
            btn.css("display", "inline-block");
            load.css("display", "none");
            aviso.text(jqXHR.responseJSON.msg);
            switch (jqXHR.responseJSON.estado) {
                case 1:
                    modal.css("display", "block");
                    modal_form.css("display", "none");
                    modal_text.text(jqXHR.responseJSON.msg);
                    $("#formulario_aviso").text("");
                    break;
                case 2:
                    aviso.css("display", "block");
                    aviso.text(jqXHR.responseJSON.msg);
                    $("#formulario_aviso").text(jqXHR.responseJSON.msg);
                    captcha.css("border", "1px solid red");
                    break;
                case 4:
                    aviso.css("display", "block");
                    aviso.text(jqXHR.responseJSON.msg);
                    break;
                case 5:
                    aviso.css("display", "block");
                    aviso.text("Debes llenar todos los campos");
                    break;
            }
        }
    });
});
$('#refreshCaptcha').click(function () {
    $('#imageCaptcha').attr('src', '/Desarrollo/CaptchaImage/' + new Date().getTime());
});
function alfa(valor) {
    var reg = /^([a-zA-Z _-]+)$/;
    return reg.test(valor);
}
function alfanumerico(valor) {
    var reg = /^([a-zA-Z0-9 _-.]+)$/;
    return reg.test(valor);
}
function numerico(valor) {
    var salida = true;
    for (i in valor) {
        var n = parseInt(valor[i]);
        if (!(n >= 0 && n <= 9)) {
            salida = false;
        }
    }
    return salida;
}
function val_email(email) {
    var re = /^(([^<>()\[\]\\.,;:\s@"]+(\.[^<>()\[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(String(email).toLowerCase());
}
