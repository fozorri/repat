                $(".js-submenu-trigger-5").removeClass("is-open")

        }), $(".js-submenu-trigger-4").mouseover(function (e) {
            //e.preventDefault(),
            $(".c-submenu-4").addClass("is-open"),
                $(".c-submenu-2").removeClass("is-open"),
                $(".c-submenu-1").removeClass("is-open"),
                $(".c-submenu-3").removeClass("is-open"),
                $(".c-submenu-5").removeClass("is-open"),
                $(".js-submenu-trigger-4").addClass("is-open"),
                $(".js-submenu-trigger-3").removeClass("is-open"),
                $(".js-submenu-trigger-2").removeClass("is-open"),
                $(".js-submenu-trigger-1").removeClass("is-open"),
                $(".js-submenu-trigger-5").removeClass("is-open")

        }), $(".js-submenu-trigger-5").mouseover(function (e) {
            //e.preventDefault(),
            $(".c-submenu-5").addClass("is-open"),
                $(".c-submenu-1").removeClass("is-open"),
                $(".c-submenu-2").removeClass("is-open"),
                $(".c-submenu-3").removeClass("is-open"),
                $(".c-submenu-4").removeClass("is-open"),
                $(".js-submenu-trigger-5").addClass("is-open"),
                $(".js-submenu-trigger-1").removeClass("is-open"),
                $(".js-submenu-trigger-2").removeClass("is-open"),
                $(".js-submenu-trigger-3").removeClass("is-open"),
                $(".js-submenu-trigger-4").removeClass("is-open")

        }), $(".slick-carousel").slick({
            infinite: true,
            arrows: !0,
            dots: !1,
            centerPadding: "200px",
            slidesToShow: 1,
            slidesToScroll: 1,
            autoplay: true,
            speed: 700,
            autoplaySpeed: 8000,
            cssEase: 'ease-out',
            pauseOnFocus: false,
            pauseOnHover: false


        }), $("#bcp_slider").slick({
            infinite: true,
            fade: true,
            arrows: true,
            autoplay: true,
            speed: 700,
            autoplaySpeed: 4000,
            pauseOnFocus: false,
            pauseOnHover: false,
            responsive: [
                {
                    breakpoint: 640,
                    settings: {
                        arrows: false,
                    }
                }
            ]
        }), $('a[href*="#"]').not('[href="#"]').not('[href="#0"]').click(function (e) {
            if (location.pathname.replace(/^\//, "") == this.pathname.replace(/^\//, "") && location.hostname == this.hostname) {
                var t = $(this.hash);
                t = t.length ? t : $("[name=" + this.hash.slice(1) + "]"), t.length && (e.preventDefault(), $("html, body").animate({
                    scrollTop: t.offset().top
                }, 1e3, function () {
                    var e = $(t);
                    return e.focus(), e.is(":focus") ? !1 : (e.attr("tabindex", "-1"), void e.focus())
                }))
            }
        })
    });

