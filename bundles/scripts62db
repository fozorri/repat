function verOficinaLaPaz() {
    $("#fondoOficinaLaPaz").css("display", "block");
    $("html").css("overflow-y", "hidden")
}
function verOficinaLaPaz() {
    $("#fondoOficinaLaPaz").css("display", "block");
    $("html").css("overflow-y", "hidden")
}

function cerrarOficinasLaPaz() {
    $("#fondoOficinaLaPaz").css("display", "none");
    $("html").css("overflow-y", "auto");
}
function cerrarComunidad() {
    $("#fondoOficinaLaPaz").css("display", "none");
    $("html").css("overflow-y", "auto");
}
