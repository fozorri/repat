//**************** animaciones poligonales **************
isAnime = false;
if (typeof anime != "undefined") {
    isAnime = true;
}

var bcp_formas_class = function (p_inicial, p_destino, objetivos) {
    viewbox = "0 0 362 318";
    imagenWidth = "362";//362 valor de produccion
    imageHeight = "318";//318 valor de produccion
    prefijo_pathId = "shape_polygon_";
    prefijo_clipId = "shape_clippath_";

    this.setViewBox = function (valor) { viewbox = valor }
    this.setImagenWidth = function (valor) { imagenWidth = valor }
    this.setImageHeight = function (valor) { imageHeight = valor }
    this.setPrefijo_pathId = function (valor) { prefijo_pathId = valor }
    this.setPrefijo_clipId = function (valor) { prefijo_clipId = valor }

    this.generar = function () {

        objetivos.each(function (obj) {


            var url = $(objetivos[obj]).attr("data-url");
            var pathid = prefijo_pathId + obj;
            var clipid = prefijo_clipId + obj;
            //var viewbox = this.viewbox;

            var clipPath = '<clipPath id="' + clipid + '"><polygon id="' + pathid + '" fill="#FFFFFF" points="' + p_inicial + '" ></polygon></clipPath>';

            var svg = '<svg class="clip-svg" viewBox="' + viewbox + '" overflow="visible"  ><image class="svg-images" width="' + imagenWidth + '" height="' + imageHeight + '" xlink:href="' + url + '" clip-path="url(#' + clipid + ')" />' + clipPath + '</svg>';
            $(objetivos[obj]).append(svg);

            if (isAnime) {
                var animar = anime({
                    easing: 'easeOutQuad',
                    targets: '#' + pathid,
                    points: [
                        {
                            value: p_destino
                        }
                    ],
                    duration: 200,
                    autoplay: false
                });

                $($(objetivos[obj]).parent()).mouseenter(function () {
                    animar.play();
                    if (animar.reversed) {
                        animar.reverse();
                    };

                });
                $($(objetivos[obj]).parent()).mouseleave(function () {
                    animar.play();
                    animar.reverse();

                });
            }

        });
    }


}

