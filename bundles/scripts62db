var puntos_inicial = '121.8,310.2 114.5,309.4 107.6,306.9 101.5,302.9 96.3,297.5 53.1,240.2 14.7,189.2 10.6,182.1   8.5,174.5 8.4,166.5 10.3,158.6 19.7,133.6 39.6,80.9 47.2,60.6 53.5,43.9 57.7,36.2 63.7,30 71.1,25.6 79.6,23.4 94.5,21.7   127.4,17.8 165.8,13.3 194.6,9.9 204,8.8 208.3,9.1 212.6,9.9 216.7,11.4 254.9,34.3 304.9,64.4 339,84.9 347.2,92 352.5,101.1   354.5,111.5 352.9,122.2 341.4,157.4 319.5,224.7 307,263 303.1,271 297.4,277.4 290.1,282.1 281.7,284.7 239.2,291.6 203.7,297.3   166.8,303.3 124.4,310.1 ';

var puntos_destino1 = '162.7,315.5 132.2,308.9 107.7,299.9 88.7,290 72.9,278.3 57.8,265.9 41.2,246.6 27.2,223.4   20.5,206.7 14.7,187.7 12.2,170.3 11.7,150 16,125.9 22.8,105.3 31.9,85.8 41.6,70.8 53.7,56.3 67.7,43.6 82.5,32 99.9,21.8   119.2,13.4 137.9,7.3 161,3.7 186.3,2.2 207.4,4 221,6.3 243.4,13 262.3,21.4 285.6,35.6 302.8,49.9 317.3,66.1 328.3,81.1   338,97.1 345.3,115.3 349.7,137 350.7,164.3 347.7,188.9 341.3,212 330,235.3 315,256.3 296.1,274.8 276.6,288.9 254.1,300.9   225.1,310.8 193.2,315.3 ';

var objetivos = $(".svgObjetivo");

var enlace_formas = new bcp_formas_class(puntos_inicial, puntos_destino1, objetivos);
enlace_formas.generar();

var enlace_forma_destacado = new bcp_formas_class('221,333.6 115.8,304.3 10.6,274.9 7.7,273.9 5.4,271.7 4.2,268.9 4.2,265.8 15.3,193.6 32.7,88.2   34.9,84.4 39,82.3 193.9,50.6 350.2,24.9 354.6,25.6 357.3,27.7 358.8,31.4 358.3,35.3 281.9,209.8 229,329 226.2,332.4 ', '243,340.6 112,316 13,291 10.1,290 7.8,287.9 6.6,285 6.6,281.9 25,180 42.8,71.8 45,68 49.1,65.9   165.6,53.1 322.9,37.6 327.3,38.3 330,40.5 331.5,44.1 331,48 286,207 251,336 248.2,339.3 ', $(".svgDestacado"));
enlace_forma_destacado.setPrefijo_pathId("path_destacado");
enlace_forma_destacado.setPrefijo_clipId("clip_destacado");
enlace_forma_destacado.generar();


//animacion logo BCP
if (isAnime) {
    var timeline = anime.timeline();
    timeline.add({
        targets: '#bcp_dibujo_svg',
        easing: 'easeOutQuint',
        opacity: [{ value: '1' }],
        duration: 100
    })
        .add({
            targets: '#bcp_dibujo_svg .bcp_shape_lineas path',
            strokeDashoffset: [anime.setDashoffset, 0],
            easing: 'easeInQuad',
            duration: 1500
        }).add({
            targets: '#bcp_dibujo_svg .bcp_shape_lineas path',
            easing: 'easeInOutSine',
            duration: 700,
            fill: [{ value: 'rgba(256,256,256,1)' }],
            begin: function () {
                anime({
                    targets: '#bcp_titulo_bcp',
                    easing: 'linear',
                    duration: 0,
                    translateY: -50
                })
            },
            complete: function () {
                anime({
                    targets: '#bcp_titulo_bcp',
                    easing: 'easeOutQuint',
                    duration: 800,
                    opacity: [{ value: '1' }],
                    translateY: 0
                })
            }
        }).add({
            targets: '#bcp_dibujo_svg',
            easing: 'easeOutQuint',
            duration: 500,
            maxWidth: [{ value: '340px' }]
        });
} else {
    $("#BCPPrincipal").css("display", "inline-block");
    $("#bcp_titulo_bcp").css("opacity", "1");
    $("#bcp_dibujo_svg").css("display", "none");
}

//animacion logo Chat
if (isAnime) {
    var timeline2 = anime.timeline({
        easing: 'easeInCubic',
        loop: true
    });
    timeline2.add({ targets: '.bcp_chat_icono #chat_b', duration: 8000 })
        .add({ targets: '.bcp_chat_icono .chat_pp', translateY: -15, duration: 80 })
        .add({ targets: '.bcp_chat_icono .chat_pp', translateY: 0, duration: 80 })
        .add({ targets: '.bcp_chat_icono #chat_b', translateY: -15, duration: 80 })
        .add({ targets: '.bcp_chat_icono #chat_b', translateY: 0, duration: 80 })
        .add({ targets: '.bcp_chat_icono #chat_c', translateY: -15, duration: 80 })
        .add({ targets: '.bcp_chat_icono #chat_c', translateY: 0, duration: 80 })
        .add({ targets: '.bcp_chat_icono #chat_p', translateY: -15, duration: 80 })
        .add({ targets: '.bcp_chat_icono #chat_p', translateY: 0, duration: 80 })
        .add({ targets: '.bcp_chat_icono .chat_cc', translateY: -15, duration: 80 })
        .add({ targets: '.bcp_chat_icono .chat_cc', translateY: 0, duration: 80 })


    anime({
        delay: 5000,
        targets: '.bcp_chat_icono .st0',
        fill: [{ value: '#F36E29', duration: 5000 }],
        strokeWidth: [{ value: '0', duration: 5000 }],
        direction: 'alternate',
        loop: true
    });
    anime({
        delay: 5000,
        targets: '.bcp_chat_icono .st1',
        fill: [{ value: '#004B8E', duration: 5000 }],
        stroke: [{ value: '#FFFFFF', duration: 5000 }],
        strokeWidth: [{ value: '1', duration: 5000 }],
        direction: 'alternate',
        loop: true
    });
    anime({
        delay: 5000,
        targets: '.bcp_chat_icono .st3',
        fill: [{ value: '#FFF', duration: 5000 }],
        direction: 'alternate',
        loop: true
    });
} else {
    $(".bcp_chat_icono svg").css("display", "none");
    $(".bcp_chat_icono img").css("display", "block");
}

//mascaras contenidos
var bcp_formas = $(".bcp_img_tipo1, .bcp_img_tipo2,.bcp_img_tipo3, .bcp_img_tipo4,.bcp_img_tipo5,.bcp_img_tipo6");
var m_contador = 0;
bcp_formas.each(function (obj) {
    var url = $(bcp_formas[obj]).css("background-image");
    //url = url.substring(url.indexOf('http'), url.length - 2);//para obtener la url del estilo
    url = url.substring(url.indexOf('http'));
    url = url.substring(0, url.indexOf(')'));
    var poligono;
    var imagenWidth = "362";//362 valor de produccion
    var imageHeight = "318";//318 valor de produccion
    var id_mascara = "shape_mascara_" + m_contador++;

    switch ($(bcp_formas[obj]).attr("class")) {
        case "bcp_img_tipo1":
            poligono = '<path xmlns="http://www.w3.org/2000/svg" class="st0" d="M120.4,312.7c-9,0-17.6-4.3-23-11.7l-85-116.4c-5.8-7.9-7.1-18.2-3.5-27.2L54.1,42.1  c3.7-9.5,12.2-16.3,22.3-17.8L201.8,5.6c1.4-0.2,2.8-0.3,4.2-0.3c5.6,0,11.1,1.6,15.8,4.8l120,79.6c10.7,7.1,15.3,20.4,11.4,32.5  l-46.4,143.5c-3.3,10.1-11.9,17.5-22.3,19.3l-159.2,27.1C123.6,312.5,122,312.7,120.4,312.7z"/>';
            break;
        case "bcp_img_tipo2":
            poligono = '<path xmlns="http://www.w3.org/2000/svg" class="st0" d="M55.6,316.9c-7.5,0-13.8-5.5-14.8-12.9L0.8,20.2c-0.7-4.9,1.1-9.9,4.8-13.2C8.4,4.4,12,3.1,15.7,3.1  c1.2,0,2.4,0.1,3.6,0.4L237,56.8c4.3,1.1,8,4,9.9,8l112.5,230.6c2.3,4.6,2,10.1-0.8,14.5c-2.7,4.4-7.5,7-12.7,7  C346,317,55.6,316.9,55.6,316.9z"/>';
            break;
        case "bcp_img_tipo3":
            poligono = '<path xmlns="http://www.w3.org/2000/svg" class="st0" d="M21.9,317c-2.2-0.2-6.7-1.6-8.5-3c-1.4-1.1-3.3-4.1-3.9-5.7c-0.6-1.5-1-4.8-1.1-6.4  C4.7,229.4,23.5,83.7,41,13.2c0.4-1.6,1.5-4.8,2.4-6.1c1-1.4,3.6-3.7,5.1-4.5c1.4-0.7,4.5-1.4,6.1-1.4c1.8,0,5.3,1.2,7,1.9  C139.4,36.4,286.5,123.7,351.4,178c1.4,1.2,4.4,3.7,5.2,5.4c0.9,1.7,1.5,5.7,1.3,7.6c-0.2,1.9-1.7,5.6-2.9,7.1  c-1.2,1.5-4.4,3.5-6.1,4.4c-75.8,39.6-239.3,93.9-322.5,113.7C25.3,316.5,23,317.1,21.9,317z"/>';
            break;
        case "bcp_img_tipo4":
            poligono = '<path xmlns="http://www.w3.org/2000/svg" class="st0" d="M44.4,318c-8.3,0-15.3-6.3-16.1-14.7L2.3,17.6c-0.5-5,1.4-9.8,5-13.3c3-2.8,7-4.4,11.1-4.4  c0.8,0,1.6,0.1,2.5,0.2l325.2,50.3c8.8,1.4,14.8,9.5,13.5,18.3l-30.5,212c-1.1,7.4-7.1,13.2-14.6,13.8L45.9,317.9  C45.4,318,44.9,318,44.4,318z"/>';
