//Contador finalizacion
var bcp_contador = function (elemento, contador) {
    var estilos = '<style>.menu_portada .tooltip{position:relative;display:inline-block;border-bottom:1px dotted #000}.menu_portada .tooltiptext{width:140px;background-color:#e4e4e4;color:black;border-radius:6px;padding:5px;position:absolute;z-index:1;bottom:-50%;margin-left:-120px}.menu_portada .tooltiptext::after{content:"";position:absolute;top:-40%;left:50%;margin-left:-8px;border-width:8px;border-style:solid;border-color:transparent transparent #e4e4e4} </style>';
    var estilos2 = '<style>#cabecera_menu .tooltiptext{visibility: hidden;opacity: 0;width:140px;background-color:#555;color:#fff;border-radius:6px;padding:5px;position:absolute;z-index:1;bottom:-70%;margin-left:-120px}#cabecera_menu .tooltiptext::after{content:"";position:absolute;top:-42%;left:50%;margin-left:-8px;border-width:8px;border-style:solid;border-color:transparent transparent #555}#cabecera_menu .btn_nueva_extranet:hover .tooltiptext {visibility:visible;opacity: 1;} .pantallaMovil_menu .tooltiptext {display:none;} .glyphicon-chevron-right{z-index:-1;}</style>';
    $("head").append(estilos);
    $("head").append(estilos2);
    $(elemento).append('<span class="tooltiptext"><span class="fa fa-calendar"></span> La versin finalizar en: ' + contador + ' das. </span>');

}

$(document).ready(function () {

    $('.social-networks').slick(
        {
            autoplay: true,
            pauseOnFocus: false,
            pauseOnHover: true

        }
    );

    $(".js-submenu-trigger").click(function () {
        $(".c-submenu").toggleClass("is-open");
        $(this).toggleClass("is-open")
    });
    $(".js-submenu-trigger-1").mouseover(function () {
        $(".c-submenu-1").addClass("is-open");
        $(".c-submenu-2").removeClass("is-open");
        $(".c-submenu-3").removeClass("is-open");
        $(".c-submenu-4").removeClass("is-open");
        $(".c-submenu-5").removeClass("is-open");
        $(".js-submenu-trigger-1").addClass("is-open");
        $(".js-submenu-trigger-2").removeClass("is-open");
        $(".js-submenu-trigger-3").removeClass("is-open");
        $(".js-submenu-trigger-4").removeClass("is-open");
        $(".js-submenu-trigger-5").removeClass("is-open")
    });
    $("#cabecera_menu").mouseleave(function () {
        $(".c-submenu-1").removeClass("is-open");
        $(".c-submenu-2").removeClass("is-open");
        $(".c-submenu-3").removeClass("is-open");
        $(".c-submenu-4").removeClass("is-open");
        $(".c-submenu-5").removeClass("is-open");
        $(".js-submenu-trigger-1").removeClass("is-open");
        $(".js-submenu-trigger-2").removeClass("is-open");
        $(".js-submenu-trigger-3").removeClass("is-open");
        $(".js-submenu-trigger-4").removeClass("is-open");
        $(".js-submenu-trigger-5").removeClass("is-open")
    });
    $(".js-submenu-trigger-2").mouseover(function () {
        $(".c-submenu-2").addClass("is-open");
        $(".c-submenu-1").removeClass("is-open");
        $(".c-submenu-3").removeClass("is-open");
        $(".c-submenu-4").removeClass("is-open");
        $(".c-submenu-5").removeClass("is-open");
        $(".js-submenu-trigger-2").addClass("is-open");
        $(".js-submenu-trigger-1").removeClass("is-open");
        $(".js-submenu-trigger-3").removeClass("is-open");
        $(".js-submenu-trigger-4").removeClass("is-open");
        $(".js-submenu-trigger-5").removeClass("is-open")
    });
    $(".js-submenu-trigger-3").mouseover(function () {
        $(".c-submenu-3").addClass("is-open");
        $(".c-submenu-1").removeClass("is-open");
        $(".c-submenu-2").removeClass("is-open");
        $(".c-submenu-4").removeClass("is-open");
        $(".c-submenu-5").removeClass("is-open");
        $(".js-submenu-trigger-3").addClass("is-open");
        $(".js-submenu-trigger-1").removeClass("is-open");
        $(".js-submenu-trigger-2").removeClass("is-open");
        $(".js-submenu-trigger-4").removeClass("is-open");
        $(".js-submenu-trigger-5").removeClass("is-open")
    });
    $(".js-submenu-trigger-4").mouseover(function () {
        $(".c-submenu-4").addClass("is-open");
        $(".c-submenu-2").removeClass("is-open");
        $(".c-submenu-1").removeClass("is-open");
        $(".c-submenu-3").removeClass("is-open");
        $(".c-submenu-5").removeClass("is-open");
        $(".js-submenu-trigger-4").addClass("is-open");
        $(".js-submenu-trigger-3").removeClass("is-open");
        $(".js-submenu-trigger-2").removeClass("is-open");
        $(".js-submenu-trigger-1").removeClass("is-open");
        $(".js-submenu-trigger-5").removeClass("is-open")
    });
    $(".js-submenu-trigger-5").mouseover(function () {
        $(".c-submenu-5").addClass("is-open");
        $(".c-submenu-1").removeClass("is-open");
        $(".c-submenu-2").removeClass("is-open");
        $(".c-submenu-3").removeClass("is-open");
        $(".c-submenu-4").removeClass("is-open");
        $(".js-submenu-trigger-5").addClass("is-open");
        $(".js-submenu-trigger-1").removeClass("is-open");
        $(".js-submenu-trigger-2").removeClass("is-open");
        $(".js-submenu-trigger-3").removeClass("is-open");
        $(".js-submenu-trigger-4").removeClass("is-open")
    });
    $(".slick-carousel").slick({
        infinite: !0,
        arrows: !0,
        dots: !1,
        centerPadding: "200px",
        slidesToShow: 1,
        slidesToScroll: 1,
        autoplay: !0,
        speed: 700,
        autoplaySpeed: 8e3,
        cssEase: "ease-out",
        pauseOnFocus: !1,
        pauseOnHover: !1
    });
    $("#bcp_slider").slick({
        infinite: !0,
        fade: !0,
        arrows: !1,
        autoplay: !0,
        speed: 700,
        autoplaySpeed: 4e3,
        pauseOnFocus: !1,
        pauseOnHover: !1
    });
    $('a[href*="#"]').not('[href="#"]').not('[href="#0"]').click(function (n) {
        if (location.pathname.replace(/^\//, "") == this.pathname.replace(/^\//, "") && location.hostname == this.hostname) {
            var t = $(this.hash);
            t = t.length ? t : $("[name=" + this.hash.slice(1) + "]");
            t.length && (n.preventDefault(), $("html, body").animate({
                scrollTop: t.offset().top
            }, 1e3, function () {
                var n = $(t);
                return n.focus(), n.is(":focus") ? !1 : (n.attr("tabindex", "-1"), void n.focus())
            }))
        }
    });

    $('.input-number').on('input', function () {
        this.value = this.value.replace(/[^0-9]/g, '');
    });

});

