function Flecha(obj, val) {
    if (val) {
        //$(obj).css("background", "#f57d30 ").css("color", "white ");
        $(obj).css("background", "white").css("color", "black ");
        $(obj).children("span").attr("class", "glyphicon glyphicon-chevron-down");
    } else {
        $(obj).css("background", "white").css("color", "black ");
        $(obj).children("span").attr("class", "glyphicon glyphicon-chevron-right");
    }
};

$("#btnPersonas").click(function () {

    if (swPersonas == 0) {
        $("#filaPersonas").show(300);
        $("#filaPymes").hide();
        $("#filaEmpresas").hide(300);
        $("#filaInversiones").hide(300);
        $("#filaSeguros").hide(300);
        $("#filaMicrocredito").hide(300);
        swPersonas = 1; swEmpresas = 0; swInveriones = 0, swPymes = 0; swSeguros = 0;
        Flecha(this, true);
        Flecha("#btnEmpresas", false);
        Flecha("#btnInversiones", false);
        Flecha("#btnPymes", false);
        Flecha("#btnSeguros", false);
        Flecha("#btnMicrodredito", false);
    }
    else {
        $("#filaPersonas").hide(300);
        swPersonas = 0;
        Flecha(this, false);
    }
}
);

$("#btnPymes").click(function () {

    if (swPymes == 0) {
        $("#filaPymes").show(300);
        $("#filaPersonas").hide(300);
        $("#filaEmpresas").hide(300);
        $("#filaInversiones").hide(300);
        $("#filaSeguros").hide(300);
        $("#filaMicrocredito").hide(300);
        swPersonas = 0; swEmpresas = 0; swInveriones = 0, swPymes = 1, swSeguros = 0;
        Flecha(this, true);
        Flecha("#btnEmpresas", false);
        Flecha("#btnInversiones", false);
        Flecha("#btnPersonas", false);
        Flecha("#btnSeguros", false);
        Flecha("#btnMicrodredito", false);
    }
    else {
        $("#filaPymes").hide(300);
        swPymes = 0;
        Flecha(this, false);
    }
}
);

