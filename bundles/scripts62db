        {
            url: "/NuestroBanco/Historia"
        }
    ).done(function (html) {
        contenidoNuestroBanco.html(html);
    }
    );
    window.onhashchange = function () {
        if (window.location.hash == "#menu") {
            mostrarMenuMovil();
        }
        if (window.location.hash == "" || window.location.hash == null || window.location.hash == undefined) {
            if (isMenuWeb) {

                ocultarMenuMovil();


            } else if ($("#pantallaMovil").css("display") == "none") {
                history.back(-2);
            }

        }
    };
    var iOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;
    var iOS2 = !!navigator.platform && /iPad|iPhone|iPod/.test(navigator.platform);
    if (iOS || iOS2) {
        $(".bcp_img_tipo0").css("background-attachment", "inherit");
    }
}
